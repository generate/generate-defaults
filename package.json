    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "generate",
      "list": [
        "assemble",
        "generate",
        "verb"
      ]
    },
    "reflinks": [
      "assemble",
      "base-pkg",
      "common-middleware",
      "common-questions",
      "engine-base",
      "generate",
      "generate-collections",
      "template-helpers",
      "verb",
      "verb-generate-readme",
      "verb-readme-generator",
      "verb-repo-data"
    ],
    "lint": {
      "reflinks": true
    }
  }
