  "keywords": [
    "app",
    "boilerplate",
    "build",
    "cli",
    "cli-app",
    "command-line",
    "create",
    "defaults",
    "dev",
    "development",
    "framework",
    "front",
    "frontend",
    "generate",
    "generate-generator",
    "generate-plugin",
    "generategenerator",
    "generateplugin",
    "generator",
    "init",
    "initialize",
    "new",
    "plugin",
    "project",
    "projects",
    "scaffold",
    "scaffolder",
