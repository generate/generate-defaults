    "template-helpers": "^0.6.7",
    "verb-repo-data": "^0.1.11"
  },
  "devDependencies": {
    "generate": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.9",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^2.2.0",
    "mocha": "^2.5.3"
