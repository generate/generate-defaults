{
  "name": "generate-defaults",
  "description": "Use this generator to add some common default settings to your 'generate', verb, and assemble generators.",
  "version": "0.5.0",
  "homepage": "https://github.com/generate/generate-defaults",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-defaults",
  "bugs": {
    "url": "https://github.com/generate/generate-defaults/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "LICENSE",
    "README.md",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=5.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "common-middleware": "^0.4.1",
    "common-questions": "^0.3.4",
    "empty-dir": "^0.2.0",
    "engine-base": "^0.1.2",
    "extend-shallow": "^2.0.1",
    "is-valid-app": "^0.2.0",
