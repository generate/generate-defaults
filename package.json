    "template",
    "templates",
    "tool",
    "verb",
    "verb-generator",
    "verbgenerator",
    "web",
    "webapp",
    "yeoman",
    "yo"
  ],
  "verb": {
    "run": true,
    "toc": true,
    "layout": "default",
    "tasks": [
      "readme"
